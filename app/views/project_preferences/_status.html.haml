- if project.archived_by?(current_user)
  %i.fa.fa-archive.text-muted{ rel: "tooltip", data: { title: "Archived", container: "body", placement: "right" } }
- elsif project.favorited_by?(current_user)
  %i.fa.fa-star.text-accent{ rel: "tooltip", data: { title: "Favorited", container: "body", placement: "right" } }
- else
  %i.fa.fa-folder-open.text-accent{ rel: "tooltip", data: { title: "Current", container: "body", placement: "right" } }
