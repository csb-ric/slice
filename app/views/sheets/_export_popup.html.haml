.row
  .col-md-4
    .download-container
      %h3 Comma-Separated Values
      %label.checkbox{ rel: 'tooltip', data: { html: 'true', title: "<table style='background-color:transparent'><tr><th style='text-align:left;'>Subject&nbsp;</th><th style='text-align:left;'>Gender</th><th style='text-align:left;'>Age</th></tr><tr><td style='text-align:left'>T325</td><td style='text-align:left'>1: male</td><td style='text-align:left'>25 years</td></tr><tr><td style='text-align:left'>T349</td><td style='text-align:left'>2: female</td><td style='text-align:left'>30 years</td></tr></table>" } }
        = check_box_tag :csv_labeled, '1', false
        CSV Labeled
      %label.checkbox{ rel: 'tooltip', data: { html: 'true', title: "<table style='background-color:transparent'><tr><th style='text-align:left;'>Subject&nbsp;</th><th style='text-align:left;'>Gender</th><th style='text-align:left;'>Age</th></tr><tr><td style='text-align:left'>T325</td><td style='text-align:left'>1</td><td style='text-align:left'>25</td></tr><tr><td style='text-align:left'>T349</td><td style='text-align:left'>2</td><td style='text-align:left'>30</td></tr></table>" } }
        = check_box_tag :csv_raw, '1', false
        CSV Raw

  .col-md-4
    .download-container
      %h3 Statistical
      %label.checkbox
        = check_box_tag :sas, '1', false
        SAS
      %label.checkbox
        = check_box_tag :r, '1', false
        R

  .col-md-4
    .download-container
      %h3 Portable Document Format
      %label.checkbox
        = check_box_tag :pdf, '1', false
        PDF

.row
  .col-md-4
    .download-container
      %h3 Uploaded Files
      %label.checkbox
        = check_box_tag :files, '1', false
        Files

  .col-md-4
    .download-container
      %h3 Data Dictionary
      %label.checkbox
        = check_box_tag :data_dictionary, '1', false
        Data Dictionary

  .col-md-4
    = link_to "Export", '#', data: { object: 'export-data', target: '#sheets_search' }, class: 'btn btn-primary btn-block btn-lg', style: 'height: 122px;font-size: 24.5px;font-weight:300;padding-top:40px'
