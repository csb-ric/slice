<% @title = 'Exports' %>
<div class="page-header">
  <h1>
    <%= link_to @project.name, @project %> &middot; <%= @title %>
    <%= link_to "Create Export", project_sheets_path(@project, e: '1'), class: 'btn btn-mini btn-primary' %>
  </h1>
</div>

<%= form_tag project_exports_path(@project), method: :get, remote: true, id: "exports_search", class: 'form-inline', data: { object: 'form-load' } do %>
  <%= hidden_field_tag :order %>
  <%= text_field_tag 'search', params[:search], class: 'form-control' %>
  <%= submit_tag 'Search', class: 'btn btn-primary' %>
<% end %>

<div id="exports"><div class="center"><%= image_tag "contour/ajax-loader.gif" %></div></div>
