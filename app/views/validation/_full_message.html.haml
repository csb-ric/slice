.bs-callout{ id: "#{target_name}_alert_box", style: "padding-top:0px;padding-bottom:0px;#{'display:none' if variable.validator.message(value) == '' and variable.variable_type != 'date'}" }
  %strong{ id: "#{target_name}_formatted_value" }= variable.validator.formatted_value(value)
  %span{ id: "#{target_name}_message" }= variable.validator.message(value)
