<% @title = 'Subject Schedules' %>
<div class="page-header">
  <h1>
    <%= @title %>
    <%= link_to "Create Subject Schedule", new_subject_schedule_path, class: 'btn btn-xs btn-primary' %>
  </h1>
</div>

<%= form_tag subject_schedules_path, method: :get, remote: true, id: "subject_schedules_search", class: 'form-inline', data: { object: 'form-load', no_turbolink: true } do %>
  <%= hidden_field_tag :order %>
  <%= text_field_tag 'search', params[:search], class: 'form-control' %>
  <%= submit_tag 'Search', class: 'btn btn-primary' %>
<% end %>

<div id="subject_schedules"><div class="center"><%= image_tag "contour/ajax-loader.gif" %></div></div>
