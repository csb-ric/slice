%tr
  - variable.child_variables.each do |child_variable|
    %td
      - grid = Grid.new(variable_id: child_variable.id, position: (position).to_s.last(9)) unless grid

      - if Variable::TYPE.flatten.uniq.include?(grid.variable.variable_type) && grid.variable.variable_type != "grid"
        = render "design_options/show/#{grid.variable.variable_type}", design: design, design_option: design_option, response: response, disabled: true, variable: grid.variable, field_name: "variables[#{variable.id}][#{position}][#{grid.variable.id}]", position: position, grid: variable, object: grid
