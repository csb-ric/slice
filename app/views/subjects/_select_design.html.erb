<%= label :without_design_id, 'Without Design', class: 'control-label' %>
<% designs = current_user.all_viewable_designs %>
<% designs = designs.with_project(params[:project_id]) unless params[:project_id].blank? %>
<div class="controls">
  <%= select_tag :without_design_id, options_for_select([['---', nil]] + designs.order('name').collect{|d| [d.name, d.id]}, params[:without_design_id] ), data: { object: 'filter' } %>
</div>
