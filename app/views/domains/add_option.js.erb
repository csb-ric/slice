<% (0..2).each do |index| %>
  <% position = Time.now.to_i.to_s + Time.now.usec.to_s + index.to_s %>
  <% @option = { name: '', value: '', description: '' } %>
  $("#options").append("<%= escape_javascript(render partial: 'edit_option', locals: { position: position }) %>");
<% end %>

