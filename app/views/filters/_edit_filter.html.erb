<div class="row-fluid">
  <div class="span6 download-container">
    <h3><%= @variable.display_name %></h3>
    <label class="checkbox <%= 'selected' if params[:missing] == '1' %>"><%= check_box_tag :missing, '1', params[:missing] == '1' %> Include Missing</label>

  </div>

  <div class="span6 download-container">
    <div id="cool-options"><%= render partial: 'filters/cool_options' %></div>
  </div>
</div>

<div class="row-fluid">
  <div class="span4">
    <%= button_tag 'Update Filter', data: { object: 'update-filter', variable_id: @variable.id }, class: 'btn btn-primary btn-block btn-large', style: 'text-align:middle;height: 122px;font-size: 24.5px;font-weight:300' %>
  </div>

  <div class="span4">
    <%= button_tag 'Remove', data: { object: 'remove-filter', target: "#variable_#{@variable.id}" }, class: 'btn btn-danger btn-block btn-large', style: 'text-align:middle;height: 122px;font-size: 24.5px;font-weight:300' %>
  </div>

  <div class="span4">
    <%= button_tag 'Cancel', data: { object: 'hide-contour-modal' }, class: 'btn btn-block btn-large', style: 'text-align:middle;height: 122px;font-size: 24.5px;font-weight:300' %>
  </div>
</div>
