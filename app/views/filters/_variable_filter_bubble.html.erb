<% v = @project.variables.find_by_id(filter[:id]) %>
<% if v %>
  <div id="variable_<%= v.id %>" class="filter-bubble" style="overflow:hidden;white-space:nowrap" data-object='edit-filter' data-variable-id="<%= v.id %>" data-project-id="<%= v.project_id %>">
    <%= hidden_field_tag "f[][id]", v.id %>
    <%= hidden_field_tag "f[][missing]", filter[:missing], data: { name: 'missing' } %>
    <%= hidden_field_tag "f[][axis]", filter[:axis], data: { name: 'axis' } %>
    <i class="icon-move"></i>
    <% if filter[:missing] == '1' %><span class="label" style="background-color:#7aa6ff;" title="Includes Missing">m</span><% end %>
    <span class="label" title="<%= v.display_name %>" style="overflow:hidden;cursor:pointer">
      <%= v.display_name %>
    </span>
    <%#= link_to 'edit', '#', data: { object: 'edit-filter', variable_id: v.id, project_id: v.project_id }, class: 'btn btn-mini' %>
    <%#= link_to 'remove', "#", data: { object: 'remove-filter', target: "#variable_#{v.id}" }, class: 'btn btn-mini btn-danger' %>
  </div>
<% end %>
