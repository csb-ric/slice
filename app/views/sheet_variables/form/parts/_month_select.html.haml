%span.month-select-box= select_tag "#{field_name}[month]", options_for_select(Date::ABBR_MONTHNAMES[1..12].collect.with_index { |m, i| [m.to_s.upcase, i + 1] }, value[:month]), class: "form-control", disabled: disabled, data: { object: "cursor-navigatable", target_name: target_name }, include_blank: "mmm", style: "display: inline-block;"
