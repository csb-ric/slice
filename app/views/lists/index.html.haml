- @title = 'Lists'

- links = []
- links << [@randomization_scheme.name, [@project, @randomization_scheme]]
- links << ['Lists', project_randomization_scheme_lists_path(@project, @randomization_scheme)]
= render 'projects/tabbed_menu', links: links

.mini-menu-outer
  .mini-menu-left-fixed
    = render 'projects/menu_mini'
  .mini-menu-right-fluid
    %h2.underline-header
      = @title
      .per-page
        = render 'contour/layouts/per_page', per_page: 40, object_count: @lists.total_count

    = render 'lists/lists'
