- @title = "#{@domain_option.name_was} Edit"
- @menu_title = "#{@project.name} #{@title}"
- @breadcrumbs = []
- @breadcrumbs << [@domain.name, [@project, @domain]]

= render 'projects/side_nav'

- content_for :header do
  .background-gradient
  .header-container
    .container
      %h1.page-heading
        = link_to @domain_option.name_was, [@project, @domain, @domain_option]
        Edit

.dashboard-container= render 'form', domain_option: @domain_option
