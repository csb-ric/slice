<% if params[:target_name].blank? %>
  $('#variables_<%= @variable.id %>').val("<%= escape_javascript(@result) %>");
  $('#variables_<%= @variable.id %>_calculation_result').val("<%= escape_javascript(@result) %>");
<% else %>
  $('#<%= params[:target_name] %>').val("<%= escape_javascript(@result) %>");
  $('#<%= params[:target_name] %>_calculation_result').val("<%= escape_javascript(@result) %>");
<% end %>
