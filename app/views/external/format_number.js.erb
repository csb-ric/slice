<% if params[:target_name].blank? %>
  $('#variables_<%= @variable.id %>').val("<%=j @result %>");
  $('#variables_<%= @variable.id %>_calculation_result').val("<%=j @result %>");
<% else %>
  $('#<%= params[:target_name] %>').val("<%=j @result %>");
  $('#<%= params[:target_name] %>_calculation_result').val("<%=j @result %>");
<% end %>
