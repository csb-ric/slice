- @breadcrumbs << [@randomization_scheme.name, [@project, @randomization_scheme]]
- @breadcrumbs << [@stratification_factor.name, [@project, @randomization_scheme, @stratification_factor]]

= render 'projects/side_nav'

- content_for :header do
  .background-gradient
  .header-container
    .container
      %h1.page-heading
        = render 'layouts/side_nav_header_button'
        = link_to @stratification_factor_option.name_was, [@project, @randomization_scheme, @stratification_factor, @stratification_factor_option]
        Edit

.dashboard-container= render 'form'
