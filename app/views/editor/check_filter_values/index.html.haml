- @title = "Values"
- @menu_title = "#{@project.name} #{@title}"
- @breadcrumbs = []
-# @breadcrumbs << ["Checks", editor_project_checks_path(@project)]
- @breadcrumbs << [@check.name, editor_project_check_path(@project, @check)]
-# @breadcrumbs << ["Filters", editor_project_check_check_filters_path(@project, @check)]
- @breadcrumbs << [@check_filter.name, editor_project_check_check_filter_path(@project, @check, @check_filter)]

- content_for :sidebar, render("projects/sidebar")

- content_for :header do
  .float-right
    .per-page= render "layouts/per_page", per_page: 40, object_count: @check_filter_values.total_count
  = @title

.dashboard-container.dashboard-table
  %table.table.table-striped.table-borderless.table-hover.table-sticky
    %col
    %col.d-none.d-sm-table-column
    %thead
      %tr
        %th Value
        %th.d-none.d-sm-table-cell Creator

    %tbody
      - @check_filter_values.each do |check_filter_value|
        %tr
          %td= link_to check_filter_value.value, editor_project_check_check_filter_check_filter_value_path(@project, @check, @check_filter, check_filter_value)
          %td.d-none.d-sm-table-cell= check_filter_value.user.full_name
