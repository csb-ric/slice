<% if @design %>
  <% @design.design_options.includes(:variable, :section).each do |design_option| %>
    <% if section = design_option.section %>
      $('#section_<%= section.id if section %>').data('branching-logic', <%= raw @design.branching_logic(design_option) %>);
    <% elsif variable = design_option.variable %>
      $('#varvar_<%= variable.id %>').data('branching-logic', <%= raw @design.branching_logic(design_option) %>);
    <% end %>
  <% end %>
<% end %>

initializeSheet();

<% if @sheet.new_record? %>
if($("#sheet_subject_id").val() != '' && $("#sheet_subject_id").val() != undefined)
  $("#design *:input[type!=hidden]:first").focus();
<% end %>
