- blank_responses = responses.select(&:blank?)
- valid_responses = responses.reject(&:blank?)

%table.table.table-fixed.table-borderless
  %thead
    %tr
      %th.center N
      %th.center.text-muted Blank
      %th.center Total
  %tbody
    %tr
      %td.center= filter_link Sheet.array_count(valid_responses), @design, variable, ':any'
      %td.center= filter_link Sheet.array_count(blank_responses), @design, variable, ':blank'
      %td.center= filter_link Sheet.array_count(responses), @design, nil, nil
