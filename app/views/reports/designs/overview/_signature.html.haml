- blank_responses = responses.select(&:blank?)
- valid_responses = responses.reject(&:blank?)

%table.table
  %thead
    %tr
      %th N
      %th.text-muted Blank
      %th Total
  %tbody
    %tr
      %td= filter_link Sheet.array_count(valid_responses), @design, variable, ':any'
      %td= filter_link Sheet.array_count(blank_responses), @design, variable, ':blank'
      %td= filter_link Sheet.array_count(responses), @design, nil, nil
